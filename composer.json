{
    "name": "astro-store/support",
    "description": "This is my package support",
    "keywords": [
        "astro-store",
        "laravel",
        "support"
    ],
    "homepage": "https://github.com/astro-store/support",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AstroStore\\Support\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AstroStore\\Support\\SupportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
